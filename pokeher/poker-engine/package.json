{
  "name": "poker-engine",
  "version": "1.0.0",
  "description": "A robust, scalable poker engine with real-time multiplayer capabilities",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node sophisticated-engine-server.js",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prebuild": "npm run clean",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "poker",
    "game",
    "real-time",
    "multiplayer",
    "websocket"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "undici": "^7.13.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "socket.io-client": "^4.8.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
